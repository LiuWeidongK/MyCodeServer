<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>teacherServ</servlet-name>
        <servlet-class>Servlet.teacherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>teacherServ</servlet-name>
        <url-pattern>/teacherServ</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>studentServ</servlet-name>
        <servlet-class>Servlet.studentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>studentServ</servlet-name>
        <url-pattern>/studentServ</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>resultServlet</servlet-name>
        <servlet-class>Servlet.resultServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>resultServlet</servlet-name>
        <url-pattern>/resultServ</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>resWebServlet</servlet-name>
        <servlet-class>Servlet.resWebServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>resWebServlet</servlet-name>
        <url-pattern>/resWebServ</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>TeacherRegServlet</servlet-name>
        <servlet-class>Servlet.TeacherRegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeacherRegServlet</servlet-name>
        <url-pattern>/teacherRegServ</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>StudentRegServlet</servlet-name>
        <servlet-class>Servlet.StudentRegServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StudentRegServlet</servlet-name>
        <url-pattern>/studentRegServ</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/loginServ</url-pattern>
    </servlet-mapping>
</web-app>